[[$head]]
    </head>
    <body>
        <div class="wrapper internal clearfix branches" id="home">
[[$header]]
            <div class="wrapper-title">
                <section class="clearfix title central">
                    <h1>[[*pagetitle]]</h1>
                </section>
            </div>
            <div class="wrapper-branches">
                <section class="clearfix branches-open central">
                    <div class="branches-list">
                        <ul class="tabs clearfix">
                            [[!getResources?
                                &parents=`[[*id]]`
                                &resources=`9,10,11,12,13`
                                &limit=`5`
                                &depth=`0`
                                &processTVs=`1`
                                &includeTVs=`1`
                                &includeContent=`1`
                                &tvPrefix=``
                                &showUnpublished=`0`
                                &showHidden=`0`
                                &sortby=`{"menuindex":"ASC","publishedon":"ASC","createdon":"ASC"}`
                                &tplPath=`[[++assets_path]]chunks/`
                                &tpl=`@FILE branches-list-item.html`
                                &tplFirst=`@FILE branches-list-first-item.html`
                                &where=`['template'=>'2']`
                             ]]
                        </ul>
                    </div>
                    <div class="panes clearfix">
                        [[!getResources?
                            &parents=`[[*id]]`
                            &resources=`9,10,11,12,13`
                            &limit=`5`
                            &depth=`0`
                            &processTVs=`1`
                            &includeTVs=`1`
                            &includeContent=`1`
                            &tvPrefix=``
                            &showUnpublished=`1`
                            &showHidden=`1`
                            &sortby=`{"menuindex":"ASC","publishedon":"ASC","createdon":"ASC"}`
                            &tplPath=`[[++assets_path]]chunks/`
                            &tpl=`@FILE branches-locations-list-item.html`
                            &where=`['template'=>'2']`
                         ]]
                    </div>
                </section>
            </div>

[[$footer]]
            <a href="#home" target="_self" title="Subir" class="boton-subir">Subir</a>
        </div>
[[$scripts]]
    </body>
</html>
